{
  "manifest_version": 2,

  "name": "Tokyo Listings",
  "description": "Companion Chrome extension for my Tokyo Listings web app",
  "version": "0.1.0",

  "content_scripts": [{
    "js": ["./dist/bundle.js"],
    "matches": ["https://realestate.yahoo.co.jp/rent/search/*","https://www.google.com/search*"]
  }],

  "icons": {
    "16": "/icons/logo16.png",
    "32": "/icons/logo32.png",
    "48": "/icons/logo48.png",
    "128": "/icons/logo128.png"
  }
}
